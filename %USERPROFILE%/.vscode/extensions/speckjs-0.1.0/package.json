{
  "name": "speckjs",
  "displayName": "Speck.js",
  "description": "Language support for Speck.js - The AI-Native Web Framework",
  "version": "0.1.0",
  "publisher": "speckjs",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "keywords": [
    "speck",
    "speckjs",
    "reactive",
    "web framework",
    "components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/petermpagano/speckjs"
  },
  "contributes": {
    "languages": [
      {
        "id": "speck",
        "aliases": ["Speck", "speck"],
        "extensions": [".speck"],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/file-icon.png",
          "dark": "./icons/file-icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "speck",
        "scopeName": "source.speck",
        "path": "./syntaxes/speck.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "speck",
        "path": "./snippets/speck.json"
      }
    ]
  }
}
